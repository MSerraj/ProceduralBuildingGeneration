    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Jupyter Notebook Debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYDEVD_IPYTHON_COMPATIBLE": "1",
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "5"
            },
            "python": "C:/Users/mehds/anaconda3/envs/SEMPRO/python.exe" // Path to your Conda Python
        }
    ]
}